(function (doc, scriptPath) { var sc, node, today=new Date(), dd=today.getDate(), mm=today.getMonth()+1, yyyy=today.getFullYear(), hh= today.getHours(), mn= today.getMinutes();	if(dd<10)dd='0'+dd;	if(mm<10)mm='0'+mm; if(hh<10)hh='0'+hh;	(mn<30) ? mn='00' : mn = '30';	today=yyyy+''+mm+''+dd+''+hh+''+mn;	var head = document.head;	var script = document.createElement("script");	script.setAttribute("id", "wdgtscr");	script.setAttribute("src", scriptPath + "&"+today);	script.setAttribute('type','application/javascript');	script.setAttribute('async',true);	head.appendChild(script);})(document, "//cdn.fishbowlprizes.com/efbv2/efb.js?wg=EeisIWpbkJ");